{
  "name": "k2ss.info",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "postbuild": "mv build/404/index.html build/404.html && svelte-sitemap --domain https://k2ss.info --trailing-slashes --ignore '**/404**'",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest",
    "biome": "biome format ./src",
    "lint": "npm run check && npm run biome",
    "format": "npm run biome -- --write"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/kit": "^2.20.7",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@testing-library/svelte": "^5.2.7",
    "autoprefixer": "^10.4.21",
    "contentful": "^10.15.1",
    "dayjs": "^1.11.13",
    "jsdom": "^26.1.0",
    "marked": "^11.1.0",
    "postcss": "^8.5.3",
    "svelte": "^5.27.0",
    "svelte-check": "^4.1.7",
    "svelte-fa": "^4.0.4",
    "svelte-meta-tags": "^4.2.0",
    "svelte-sitemap": "^2.7.0",
    "tailwindcss": "^3.4.17",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "vite": "^6.2.6",
    "vitest": "^3.1.3"
  },
  "engines": {
    "node": ">=20 <21"
  },
  "overrides": {
    "@asamuzakjp/css-color": "^2.8.3-b.2"
  },
  "type": "module"
}
